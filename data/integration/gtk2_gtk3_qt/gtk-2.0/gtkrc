		}
		# Used for dialog bg. in apps. like Inkscape 
		image {
			function		= BOX
			recolorable	= TRUE
			file        	= "null.png"
			border      	= { 2, 2, 2, 2 }
			stretch        	= TRUE
		}
	}
}

