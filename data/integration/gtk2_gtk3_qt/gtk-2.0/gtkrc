style "styleStatusBar" {
	# Grip in the bottom right corner to resize a box
	engine "pixmap" {
		image {
			function = RESIZE_GRIP
			detail = "statusbar"
			overlay_file = "../img2/colorset5/resizer.png"
			overlay_stretch	= FALSE
		}
	}
}

