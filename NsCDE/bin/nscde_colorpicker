#!/bin/ksh

#
# This file is a part of the NsCDE - Not so Common Desktop Environment
# Author: Hegel3DReloaded
# Licence: GPLv3
#

[ -z $NSCDE_ROOT ] && exit 2
MARCH=$(uname -m)
BIN="$NSCDE_ROOT/bin/colorpicker-${NSCDE_OS}_${MARCH}"

Existance=$(type "$BIN" 2>&1)

if [ -x "$BIN" ]; then
   exec "$BIN" "$@"
else
   echo "$Existance"
   exit 1
fi

