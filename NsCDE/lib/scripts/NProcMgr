Widget 54
Property
   Size 284 20
   Position 232 612
   Type PushButton
   Flags NoReliefString
   LocaleTitle {Reread}
   Main
      Case message of
      SingleClic :
      Begin
         SendSignal 54 1
      End
      1 :
      Begin
         Set $ReReadCfg = (GetValue 53)

         If $ReReadCfg == 1 Then
            Do {f_ReadCfg Animate}
         If $ReReadCfg == 2 Then
            Do {f_ReadCfg Backdrops}
         If $ReReadCfg == 3 Then
            Do {f_ReadCfg Backer}
         If $ReReadCfg == 4 Then
            Do {f_ReadCfg Colorset}
         If $ReReadCfg == 5 Then
            Do {f_ReadCfg Event}
         If $ReReadCfg == 6 Then
            Do {f_ReadCfg Functions}
         If $ReReadCfg == 7 Then
            Do {f_ReadCfg IconMan}
         If $ReReadCfg == 8 Then
            Do {f_ReadCfg Ident}
         If $ReReadCfg == 9 Then
            Do {f_ReadCfg Keybindings}
         If $ReReadCfg == 10 Then
            Do {f_ReadCfg Menus}
         If $ReReadCfg == 11 Then
            Do {f_ReadCfg Mousebindings}
         If $ReReadCfg == 12 Then
            Do {f_ReadCfg Pager-GlobalPager}
         If $ReReadCfg == 13 Then
            Do {f_ReadCfg Pager-LocalPager}
         If $ReReadCfg == 14 Then
            Do {f_ReadCfg Pager-WspLocPager}
         If $ReReadCfg == 15 Then
            Do {f_ReadCfg Script}
         If $ReReadCfg == 16 Then
            Do {f_ReadCfg Style}
      End
End


