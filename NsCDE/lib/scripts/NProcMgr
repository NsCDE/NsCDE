      End
      3 :
      Begin
         Do {f_RestartFrontPanel}
      End
      4 :
      Begin
         Do {KillModule FvwmButtons FrontPanel}
         Do {Schedule 100 All (FrontPanel, CirculateHit) Deschedule $[w.id]}
      End
      5 :
      Begin
         Do {Schedule 100 ModuleSynchronous FvwmButtons FrontPanel}
      End
End

